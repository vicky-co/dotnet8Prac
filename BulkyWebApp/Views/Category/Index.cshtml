@model List<Category>

    <h1>List of categories</h1>
    <table class="table-bordered table ">
       <thead>
           <th>
               Name
           </th>
           <th>
               Display Order
           </th>
       </thead>
       <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Name
                </td>
                <td>
                    @item.DisplayOrder
                </td>
            </tr>
        }
       </tbody>
</table>